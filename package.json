{
  "name": "rua",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "scripts": {
    "new": "node ./scripts/newModule.js",
    "start": "node ./scripts/dev.js",
    "format": "yarn run format:js && yarn run format:css",
    "format:js": "eslint --fix --color ./components/**/*.js ./components/**/*.jsx",
    "format:css": "stylelint --fix --color ./components/**/*.scss",
    "build": "yarn run build:es && yarn run build:commonjs && yarn run build:webpack",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./components --ignore *.test.js --out-dir ./es",
    "build:commonjs": "cross-env NODE_ENV=production babel ./components --ignore *.test.js --out-dir ./commonjs",
    "build:webpack": "webpack --config './webpack.prod.js' --color",
    "dashboard": "webpack-dashboard -p 8081 -c",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format -- ",
      "git add"
    ]
  },
  "repository": "https://github.com/itcat99/rua.git",
  "author": "FreMaNgo <boping2010@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.1",
    "execa": "^0.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "node-sass": "^4.9.0",
    "prettier": "1.12.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
